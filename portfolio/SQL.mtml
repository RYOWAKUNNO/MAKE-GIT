class Car 
{private static int sum=0;
private double gas;
public Car(){
num=0;
gas=0.0;
sum++;
System.out.println("車を作成しました。");
}
public void setCar(int n,double g){
num=n;
gas=g;
System.outprintln("車のナンバーを"+num+"ガソリンの量を"+gas+"にしました");
System.out.println("車のナンバーは"+num+"です");
System.out.println("ガソリンの量は"+gas+"です");
}
}